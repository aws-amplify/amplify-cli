{
  "name": "graphql-relational-schema-transformer",
  "version": "2.13.1",
  "description": "An AppSync model transform that takes a relational database and turns that into a GraphQL API.",
  "main": "lib/index.js",
  "scripts": {
    "test-ci": "jest --ci --coverage",
    "test": "jest --coverage",
    "build": "tsc",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "cloudform-types": "^4.1.0",
    "fs-extra": "^8.1.0",
    "graphql": "^14.5.8",
    "graphql-mapping-template": "4.13.1",
    "graphql-transformer-common": "4.13.1"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/jest": "23.1.1",
    "aws-sdk": "^2.608.0",
    "cloudform": "^4.1.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "~3.6.4"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
