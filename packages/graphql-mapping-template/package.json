{
  "name": "graphql-mapping-template",
  "version": "4.13.1",
  "description": "An AST wrapper around AWS AppSync resolvers",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "appsync",
    "cloudformation",
    "aws"
  ],
  "devDependencies": {
    "@types/jest": "22.2.3",
    "jest": "^23.0.0",
    "ts-jest": "^22.4.6",
    "typescript": "~3.6.4"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
