    "@aws-cdk/core": "~1.72.0",
    "constructs": "^3.0.12",
    "graphql": "^14.5.8",
    "graphql-mapping-template": "4.18.2",
    "graphql-transformer-common": "4.19.7",
    "lodash": "^4.17.21"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/node": "^12.12.6"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
