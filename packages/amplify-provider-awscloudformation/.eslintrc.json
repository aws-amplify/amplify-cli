{
    "extends": "airbnb-base",
    "rules": {
    	"no-param-reassign": "off",
        "no-shadow": "off",
    	"no-return-await" : "off",
		"no-continue": "off",
		"no-plusplus": "off",
    	"no-await-in-loop": "off",
    	"no-use-before-define": "off",
    	"no-console": "off",
    	"global-require": "off",
    	"import/no-dynamic-require": "off",
    	"consistent-return": "off"
    }
}