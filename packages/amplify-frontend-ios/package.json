{
  "name": "amplify-frontend-ios",
  "version": "3.6.0-beta.0",
  "description": "amplify-cli front-end plugin for xcode projects",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-frontend-ios"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "index.js",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "generate-xcode-bindings": "node ./scripts/native-bindings-gen",
    "test": "jest --logHeapUsage",
    "test-watch": "jest --watch"
  },
  "dependencies": {
    "amplify-cli-core": "4.0.0-beta.5",
    "execa": "^5.1.1",
    "fs-extra": "^8.1.0",
    "graphql-config": "^2.2.1",
    "lodash": "^4.17.21"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "lib/**/*.{ts,tsx,js,jsx}",
      "!lib/__tests__/"
    ],
    "testURL": "http://localhost",
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
