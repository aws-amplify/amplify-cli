{
  "name": "graphql-transformers-e2e-tests",
  "version": "5.0.0",
  "description": "End to end functional tests for appsync supported transformers.",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "e2e": "jest",
    "build": "tsc",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "amazon-cognito-identity-js": "^3.0.7",
    "axios": "^0.19.0",
    "cloudform": "^3.5.0",
    "graphql": "^0.13.2",
    "graphql-mapping-template": "3.7.0",
    "graphql-transformer-common": "3.14.0",
    "graphql-transformer-core": "5.0.0",
    "moment": "^2.24.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.4",
    "@types/jest": "23.1.1",
    "@types/node": "^8.10.51",
    "aws-amplify": "^1.1.36",
    "aws-sdk": "^2.510.0",
    "graphql-auth-transformer": "5.0.0",
    "graphql-connection-transformer": "3.11.0",
    "graphql-dynamodb-transformer": "5.0.0",
    "graphql-elasticsearch-transformer": "3.12.0",
    "graphql-function-transformer": "1.7.0",
    "graphql-key-transformer": "1.7.0",
    "graphql-versioned-transformer": "3.10.0",
    "jest": "^23.1.0",
    "node-fetch": "^2.6.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "testPathIgnorePatterns": [
      "/lib/",
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
