{
  "name": "@aws-amplify/cli",
  "version": "3.0.0",
  "description": "Amplify CLI",
  "bin": {
    "amplify": "bin/amplify"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "lint": "eslint src",
    "lint-fix": "eslint . --fix",
    "postinstall": "node scripts/post-install.js"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "amplify-category-analytics": "1.11.0",
    "amplify-category-api": "1.13.0",
    "amplify-category-auth": "1.13.0",
    "amplify-category-function": "1.13.0",
    "amplify-category-hosting": "1.8.0",
    "amplify-category-interactions": "1.12.0",
    "amplify-category-notifications": "1.10.0",
    "amplify-category-predictions": "1.6.0",
    "amplify-category-storage": "1.14.0",
    "amplify-category-xr": "1.12.0",
    "amplify-codegen": "1.11.0",
    "amplify-frontend-android": "1.14.0",
    "amplify-frontend-ios": "1.14.0",
    "amplify-frontend-javascript": "1.12.0",
    "amplify-provider-awscloudformation": "3.0.0",
    "amplify-util-mock": "2.0.0",
    "babel-core": "^6.26.3",
    "chalk": "^2.4.2",
    "chalk-pipe": "^2.0.0",
    "env-editor": "^0.4.0",
    "esm": "^3.2.25",
    "figlet": "^1.2.1",
    "folder-hash": "^2.0.0",
    "fs-extra": "^8.1.0",
    "gluegun": "3.3.2",
    "graphql": "^0.13.2",
    "ini": "^1.3.5",
    "inquirer": "^6.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "optionator": "^0.8.2",
    "ora": "^3.4.0",
    "promise-sequential": "^1.1.1",
    "util": "^0.12.1",
    "which": "^1.3.1"
  },
  "devDependencies": {
    "babel-eslint": "^8.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^23.0.0"
  },
  "jest": {
    "collectCoverage": true,
    "testURL": "http://localhost",
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
