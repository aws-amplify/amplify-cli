{
  "name": "@aws-amplify/cli",
  "version": "4.2.0",
  "description": "Amplify CLI",
  "main": "lib/index.js",
  "bin": {
    "amplify": "bin/amplify"
  },
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "lint": "./node_modules/.bin/tslint -c tslint.json 'src/**/*.ts' && eslint 'src/**/*.js'",
    "lint-fix": "./node_modules/.bin/tslint --fix -c tslint.json 'src/**/*.ts' && eslint 'src/**/*.js' --fix",
    "postinstall": "node scripts/post-install.js",
    "build": "tsc",
    "start": "tsc -w",
    "clean": "rm -rf ./lib"
  },
  "engines": {
    "node": ">=8.12.0"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/global-prefix": "^3.0.0",
    "amplify-category-analytics": "2.2.0",
    "amplify-category-api": "2.2.0",
    "amplify-category-auth": "2.2.0",
    "amplify-category-function": "2.2.0",
    "amplify-category-hosting": "2.0.3",
    "amplify-category-interactions": "2.0.3",
    "amplify-category-notifications": "2.2.0",
    "amplify-category-predictions": "2.0.3",
    "amplify-category-storage": "2.0.3",
    "amplify-category-xr": "2.0.3",
    "amplify-codegen": "2.2.0",
    "amplify-frontend-android": "2.2.0",
    "amplify-frontend-ios": "2.2.0",
    "amplify-frontend-javascript": "2.2.0",
    "amplify-provider-awscloudformation": "4.2.0",
    "amplify-util-mock": "3.2.0",
    "babel-core": "^6.26.3",
    "chalk": "^2.4.2",
    "chalk-pipe": "^2.0.0",
    "cli-table3": "^0.5.1",
    "colors": "^1.3.3",
    "ejs": "^2.6.2",
    "env-editor": "^0.4.0",
    "esm": "^3.2.25",
    "extract-zip": "^1.6.7",
    "figlet": "^1.2.1",
    "folder-hash": "^2.0.0",
    "fs-extra": "^8.1.0",
    "global-prefix": "^3.0.0",
    "ini": "^1.3.5",
    "inquirer": "^6.5.1",
    "lodash": "^4.17.15",
    "mime-types": "^2.1.24",
    "moment": "^2.24.0",
    "open": "^6.4.0",
    "optionator": "^0.8.2",
    "ora": "^3.4.0",
    "promise-sequential": "^1.1.1",
    "util": "^0.12.1",
    "which": "^1.3.1",
    "xcode": "^2.0.0"
  },
  "devDependencies": {
    "@types/jest": "^24.0.15",
    "@types/node": "^12.0.4",
    "babel-eslint": "^8.0.1",
    "eslint": "^4.19.1",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.12.0",
    "jest": "^24.8.0",
    "ts-jest": "^24.0.2",
    "tslint": "^5.17.0",
    "tslint-config-airbnb-base": "^0.3.0",
    "typescript": "~3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/src/__tests__/.*|(\\.|/)test)\\.tsx?$",
    "coveragePathIgnorePatterns": [
      "/node_modules/"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
