    "@aws-amplify/amplify-cli-logger": "1.3.8",
    "@aws-amplify/amplify-cli-shared-interfaces": "1.2.6",
    "@aws-amplify/amplify-console-hosting": "2.5.40",
    "@aws-amplify/amplify-container-hosting": "2.8.20",
    "@aws-amplify/amplify-dotnet-function-template-provider": "2.7.6",
    "@aws-amplify/amplify-environment-parameters": "1.9.21",
    "@aws-amplify/amplify-frontend-android": "3.5.8",
    "@aws-amplify/amplify-frontend-flutter": "1.4.7",
    "@aws-amplify/amplify-frontend-ios": "3.7.14",
    "@aws-amplify/amplify-frontend-javascript": "3.10.24",
    "@aws-amplify/amplify-go-function-template-provider": "1.4.9",
    "@aws-amplify/amplify-nodejs-function-template-provider": "2.10.16",
    "@aws-amplify/amplify-prompts": "2.8.7",
    "@aws-amplify/amplify-provider-awscloudformation": "8.11.12",
    "@aws-amplify/amplify-python-function-template-provider": "1.4.8",
    "@aws-amplify/amplify-util-import": "2.8.5",
    "@aws-amplify/amplify-util-mock": "5.10.20",
    "@aws-amplify/amplify-util-uibuilder": "1.14.21",
