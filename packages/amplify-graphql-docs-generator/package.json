{
  "name": "amplify-graphql-docs-generator",
  "version": "2.0.5",
  "main": "lib/index.js",
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=8.12.0"
  },
  "bin": {
    "amplify-graphql-docs-generator": "./bin/cli"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib",
    "lint": "tslint -p ./tsconfig.json"
  },
  "devDependencies": {
    "@types/ejs": "^2.6.0",
    "@types/graphql": "^0.13.4",
    "@types/jest": "23.3.1",
    "@types/node": "^8.10.59",
    "@types/prettier": "^1.13.2",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.18.0",
    "typescript": "~3.6.4"
  },
  "dependencies": {
    "camel-case": "^3.0.0",
    "commander": "^2.9.0",
    "fetch": "^1.1.0",
    "graphql": "^0.13.2",
    "handlebars": "^4.1.2",
    "lodash": "^4.17.15",
    "node-fetch": "^2.6.0",
    "pascal-case": "^2.0.1",
    "prettier": "^1.18.2",
    "yargs": "^13.3.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node"
    ]
  }
}
