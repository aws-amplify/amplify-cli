{
  "name": "amplify-graphql-docs-generator",
  "version": "2.1.8",
  "main": "lib/index.js",
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": {
    "amplify-graphql-docs-generator": "./bin/cli"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "directories": {
    "test": "tests"
  },
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib"
  },
  "devDependencies": {
    "@types/jest": "23.3.1",
    "@types/node": "^10.17.13",
    "@types/prettier": "^1.19.0",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "~3.6.4"
  },
  "dependencies": {
    "change-case": "^4.1.1",
    "graphql": "^14.5.8",
    "handlebars": "4.5.0",
    "prettier": "^1.19.1",
    "yargs": "^15.1.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts"
    ],
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "node"
    ]
  }
}
