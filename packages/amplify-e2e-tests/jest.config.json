{
    "transform": {
        "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(__tests__/.*.test.*)$",
    "testPathIgnorePatterns": [
        "/node_modules/",
        "src"
    ],
