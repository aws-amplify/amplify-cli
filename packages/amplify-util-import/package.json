{
  "name": "amplify-util-import",
  "version": "1.5.6",
  "description": "Resource import support package",
  "main": "lib/index.js",
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo"
  },
  "keywords": [
    "aws",
    "amplify",
    "import"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "amplify-cli-core": "1.25.1",
    "aws-sdk": "^2.919.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.6"
  },
  "jest": {
    "collectCoverage": true,
    "coverageReporters": [
      "json",
      "html"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test\\.(js|ts))$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "globals": {
      "ts-jest": {
        "diagnostics": false
      }
    }
  }
}
