{
  "name": "@aws-amplify/amplify-category-geo",
  "version": "2.12.1",
  "description": "Amplify CLI plugin to manage the Geo resources for the project",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-category-geo"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "test": "jest --logHeapUsage",
    "watch": "tsc -w",
    "extract-api": "ts-node ../../scripts/extract-api.ts"
  },
  "keywords": [
    "amplify",
    "aws"
  ],
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "@aws-cdk/aws-iam": "~1.172.0",
    "@aws-cdk/aws-lambda": "~1.172.0",
    "@aws-cdk/core": "~1.172.0",
    "ajv": "^6.12.6",
    "amplify-cli-core": "3.7.1",
    "amplify-headless-interface": "1.17.1",
    "amplify-prompts": "2.6.4",
    "amplify-util-headless-input": "1.9.10",
    "aws-sdk": "^2.1233.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.21",
    "uuid": "^8.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/__tests__/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
