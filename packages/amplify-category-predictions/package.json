{
  "name": "amplify-category-predictions",
  "version": "2.9.15",
  "description": "amplify-cli predictions plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-category-predictions"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
