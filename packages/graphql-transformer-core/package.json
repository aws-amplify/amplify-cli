{
  "name": "graphql-transformer-core",
  "version": "3.7.7-beta.0",
  "description": "A framework to transform from GraphQL SDL to AWS CloudFormation.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "buildw": "tsc -w",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "cloudformation",
    "aws"
  ],
  "author": "Michael Paris",
  "license": "MIT",
  "devDependencies": {
    "@types/fs-extra": "^7.0.0",
    "@types/graphql": "^0.13.1",
    "@types/jest": "23.1.0",
    "@types/node": "10.3.4",
    "jest": "^23.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.10.0",
    "typescript": "^3.5.1"
  },
  "dependencies": {
    "cloudform": "^3.5.0",
    "cloudform-types": "^3.7.0",
    "deep-diff": "^1.0.2",
    "fs-extra": "^8.0.1",
    "graphql": "^0.13.2",
    "graphql-transformer-common": "3.8.5-beta.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
