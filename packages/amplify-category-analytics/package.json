{
  "name": "@aws-amplify/amplify-category-analytics",
  "version": "5.0.1",
  "description": "amplify-cli analytics plugin",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-category-analytics"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "keywords": [
    "amplify",
    "aws"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "clean": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "watch": "tsc --watch",
    "test": "jest --logHeapUsage",
    "extract-api": "ts-node ../../scripts/extract-api.ts",
    "bundle-dependencies": "bundle-dependencies update",
    "pack-packages": "npm pack"
  },
  "dependencies": {
    "@aws-amplify/amplify-environment-parameters": "file://home/akz/workspace/amplify-cli/packages/amplify-environment-parameters/aws-amplify-amplify-environment-parameters-1.4.0-beta.2.tgz",
    "amplify-cli-core": "file://home/akz/workspace/amplify-cli/packages/amplify-cli-core/amplify-cli-core-4.0.0-beta.8.tgz",
    "amplify-prompts": "file://home/akz/workspace/amplify-cli/packages/amplify-prompts/amplify-prompts-2.6.4.tgz",
    "fs-extra": "^8.1.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "collectCoverage": true,
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "bundledDependencies": [
    "@aws-amplify/amplify-environment-parameters",
    "amplify-cli-core",
    "amplify-prompts",
    "fs-extra",
    "uuid"
  ]
}