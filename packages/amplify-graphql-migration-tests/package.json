{
  "name": "amplify-graphql-migration-tests",
  "version": "2.5.1",
  "description": "Tests migration from v1 to v2 of the Amplify GraphQL transformer",
  "main": "lib/index.js",
  "private": true,
  "scripts": {
    "test": "jest --logHeapUsage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-schema-migrator-tests"
  },
  "keywords": [
    "graphql",
    "transformer",
    "migration",
    "test"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "jest": {
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/__tests__/"
    ],
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testEnvironmentOptions": {
      "url": "http://localhost"
    },
    "testRegex": "((\\.|/)(test|spec))\\.(jsx?|tsx?)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "devDependencies": {
    "@aws-amplify/amplify-prompts": "2.8.2",
    "@aws-amplify/graphql-auth-transformer": "^2.1.13",
    "@aws-amplify/graphql-default-value-transformer": "^1.1.13",
    "@aws-amplify/graphql-function-transformer": "^1.2.11",
    "@aws-amplify/graphql-http-transformer": "^1.2.11",
    "@aws-amplify/graphql-index-transformer": "^1.2.9",
    "@aws-amplify/graphql-model-transformer": "^1.4.0",
    "@aws-amplify/graphql-predictions-transformer": "^1.2.11",
    "@aws-amplify/graphql-relational-transformer": "^1.2.9",
    "@aws-amplify/graphql-searchable-transformer": "^1.2.11",
    "@aws-amplify/graphql-transformer-core": "^1.4.0",
    "@aws-amplify/graphql-transformer-interfaces": "2.3.0",
    "@aws-amplify/graphql-transformer-migrator": "^2.1.13",
    "@aws-cdk/cloudformation-diff": "~2.68.0",
    "fs-extra": "^8.1.0",
    "graphql-auth-transformer": "^7.2.63",
    "graphql-connection-transformer": "^5.2.62",
    "graphql-dynamodb-transformer": "^7.2.62",
    "graphql-elasticsearch-transformer": "^5.2.63",
    "graphql-function-transformer": "^3.3.53",
    "graphql-http-transformer": "^5.2.62",
    "graphql-key-transformer": "^3.2.62",
    "graphql-predictions-transformer": "^3.2.62",
    "graphql-transformer-core": "^8.1.9",
    "graphql-versioned-transformer": "^5.2.62"
  },
  "berry": {
    "plugins": [
      "@yarn/plugin-typescript"
    ]
  }
}
