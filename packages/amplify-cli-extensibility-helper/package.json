{
  "name": "@aws-amplify/cli-extensibility-helper",
  "version": "3.0.0-beta.8",
  "description": "Amplify CLI Extensibility Helper utility package",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-cli-extensibility-helper"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "cli",
    "cloudformation",
    "aws",
    "amplify"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "extract-api": "ts-node ../../scripts/extract-api.ts",
    "bundle-dependencies": "bundle-dependencies update",
    "pack-packages": "npm pack"
  },
  "dependencies": {
    "@aws-amplify/amplify-category-custom": "file://home/akz/workspace/amplify-cli/packages/amplify-category-custom/aws-amplify-amplify-category-custom-3.0.0-beta.8.tgz",
    "amplify-cli-core": "file://home/akz/workspace/amplify-cli/packages/amplify-cli-core/amplify-cli-core-4.0.0-beta.8.tgz",
    "aws-cdk-lib": "~2.53.0"
  },
  "jest": {
    "transform": {
      "^.+\\.(ts|tsx)?$": "ts-jest"
    },
    "collectCoverageFrom": [
      "src/**/*.{ts,tsx,js,jsx}",
      "!src/__tests__/"
    ],
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  },
  "bundledDependencies": [
    "@aws-amplify/amplify-category-custom",
    "amplify-cli-core",
    "aws-cdk-lib"
  ]
}