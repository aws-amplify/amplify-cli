GOOGLE_APP_ID=<your-google-app-id>
GOOGLE_APP_SECRET=<your-google-app-secret>

AMAZON_APP_ID=<your-amazon-app-id>
AMAZON_APP_SECRET=<your-amazon-app-secret>
