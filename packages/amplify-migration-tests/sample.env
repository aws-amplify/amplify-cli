AMPLIFY_PATH=<PATH_TO_ROOT/packages/amplify-cli/bin/amplify>

# Used for setting up a new profile
AWS_ACCESS_KEY_ID=<your-access-key>
AWS_SECRET_ACCESS_KEY=<your-secret-access-key>
