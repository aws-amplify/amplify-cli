{
  "name": "graphql-connection-transformer",
  "version": "4.13.1",
  "description": "An AppSync model transform for connecting objects.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "clean": "rm -rf ./lib"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "cloudform-types": "^4.1.0",
    "graphql": "^14.5.8",
    "graphql-dynamodb-transformer": "6.13.1",
    "graphql-key-transformer": "2.13.1",
    "graphql-mapping-template": "4.13.1",
    "graphql-transformer-common": "4.13.1",
    "graphql-transformer-core": "6.13.1"
  },
  "devDependencies": {
    "@types/jest": "23.1.1",
    "@types/node": "^10.17.13",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "~3.6.4"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost/",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
