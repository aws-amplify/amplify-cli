{
  "name": "amplify-graphql-types-generator",
  "version": "2.1.8",
  "description": "Generate API code or type annotations based on a GraphQL schema and statements",
  "main": "./lib/index.js",
  "engines": {
    "node": ">=10.0.0"
  },
  "bin": "./lib/cli.js",
  "scripts": {
    "clean": "rm -rf lib",
    "build": "tsc",
    "watch": "tsc -w",
    "test": "jest",
    "test-ci": "jest --ci -i",
    "test:smoke": "npm install && npm run build && rm -rf node_modules && npm install --prod && node ./lib/cli.js && echo 'Smoke Test Passed'"
  },
  "repository": {
    "type": "git"
  },
  "original-author": "Martijn Walraven <martijn@martijnwalraven.com>",
  "author": "Amazon Web Services",
  "contributors": [
    "Min Bi",
    "Rohan Dubal",
    "Martijn Walraven <martijn@martijnwalraven.com>",
    "Yathiraj"
  ],
  "license": "MIT",
  "devDependencies": {
    "@types/common-tags": "^1.8.0",
    "@types/glob": "^7.1.1",
    "@types/inflected": "^1.1.29",
    "@types/jest": "20.0.6",
    "@types/node": "^10.17.13",
    "@types/yargs": "^15.0.1",
    "jest": "^23.1.0",
    "ts-jest": "^22.4.6",
    "typescript": "~3.6.4"
  },
  "dependencies": {
    "@babel/generator": "7.0.0-beta.4",
    "@babel/types": "7.0.0-beta.4",
    "@types/babel-generator": "^6.25.0",
    "@types/prettier": "^1.19.0",
    "@types/rimraf": "^2.0.3",
    "babel-generator": "^6.26.1",
    "babel-types": "^6.26.0",
    "change-case": "^4.1.1",
    "common-tags": "^1.8.0",
    "core-js": "^3.6.4",
    "glob": "^7.1.6",
    "graphql": "^14.5.8",
    "inflected": "^2.0.4",
    "prettier": "^1.19.1",
    "rimraf": "^3.0.0",
    "source-map-support": "^0.5.16",
    "yargs": "^15.1.0"
  },
  "jest": {
    "setupFiles": [
      "<rootDir>/src/polyfills.js"
    ],
    "collectCoverage": true,
    "testURL": "http://localhost",
    "setupTestFrameworkScriptFile": "<rootDir>/test/test-utils/matchers.ts",
    "testMatch": [
      "**/test/**/*.(js|ts)",
      "**/test/*.(js|ts)",
      "**/__tests__/*.(js|ts)"
    ],
    "testPathIgnorePatterns": [
      "<rootDir>/node_modules/",
      "<rootDir>/lib/",
      "<rootDir>/test/fixtures/",
      "<rootDir>/test/test-utils"
    ],
    "transform": {
      "^.+\\.(ts|js)x?$": "ts-jest"
    },
    "moduleFileExtensions": [
      "ts",
      "js"
    ]
  }
}
