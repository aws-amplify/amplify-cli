{
  "name": "graphql-auth-transformer",
  "version": "5.0.0",
  "description": "Implements the @auth directive for the appsync model transform.",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "clean": "rimraf ./lib",
    "watch": "tsc -w"
  },
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "graphql": "^0.13.2",
    "graphql-mapping-template": "3.7.0",
    "graphql-transformer-common": "3.14.0",
    "graphql-transformer-core": "5.0.0"
  },
  "devDependencies": {
    "@types/graphql": "^0.13.4",
    "@types/jest": "23.1.1",
    "@types/node": "^8.10.51",
    "cloudform": "^3.5.0",
    "cloudform-types": "^3.7.0",
    "graphql-dynamodb-transformer": "5.0.0",
    "jest": "^23.1.0",
    "rimraf": "^3.0.0",
    "ts-jest": "^22.4.6",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "testURL": "http://localhost",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
