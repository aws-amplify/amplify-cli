{
  "name": "amplify-util-mock",
  "version": "3.34.12",
  "description": "amplify cli plugin providing local testing",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-util-mock"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "e2e": "jest --runInBand  --forceExit src/__e2e__/*.test.ts",
    "test": "jest src/__tests__/**/*.test.ts",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "@hapi/topo": "^5.0.0",
    "amplify-appsync-simulator": "^1.27.8",
    "amplify-category-function": "^2.36.2",
    "amplify-cli-core": "^1.31.2",
    "amplify-codegen": "^2.23.1",
    "amplify-dynamodb-simulator": "^1.19.16",
    "amplify-provider-awscloudformation": "^4.64.1",
    "amplify-storage-simulator": "1.6.1",
    "chokidar": "^3.3.1",
    "detect-port": "^1.3.0",
    "dotenv": "^8.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.21",
    "semver": "^7.3.5",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/detect-port": "^1.3.0",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.6",
    "@types/semver": "^7.1.0",
    "@types/which": "^1.3.2",
    "amplify-function-plugin-interface": "1.9.1",
    "amplify-nodejs-function-runtime-provider": "^1.6.15",
    "aws-appsync": "^2.0.2",
    "aws-sdk": "^2.963.0",
    "aws-sdk-mock": "^5.1.0",
    "axios": "^0.21.4",
    "graphql-auth-transformer": "^6.24.27",
    "graphql-connection-transformer": "^4.21.26",
    "graphql-dynamodb-transformer": "^6.22.26",
    "graphql-function-transformer": "^2.5.25",
    "graphql-key-transformer": "^2.23.26",
    "graphql-tag": "^2.10.1",
    "graphql-transformer-core": "^6.30.3",
    "graphql-versioned-transformer": "^4.17.26",
    "isomorphic-fetch": "^3.0.0",
    "jsonwebtoken": "^8.5.1",
    "uuid": "^3.4.0",
    "ws": "^7.4.6"
  },
  "jest": {
    "testRunner": "jest-circus/runner",
    "preset": "ts-jest",
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/__tests__/**",
      "!lib/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost/",
    "testRegex": "(src/(__tests__|__e2e__)/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "jest-junit": {
    "outputDirectory": "reports/junit/",
    "outputName": "js-test-results.xml",
    "usePathForSuiteName": "true",
    "addFileAttribute": "true"
  }
}
