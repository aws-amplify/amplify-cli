{
  "name": "amplify-util-mock",
  "version": "3.13.0",
  "description": "amplify cli plugin providing local testing",
  "main": "lib/index.js",
  "scripts": {
    "e2e": "jest --runInBand  --forceExit src/__e2e__/*.e2e.test.ts",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "amplify-appsync-simulator": "1.13.0",
    "amplify-category-function": "2.13.0",
    "amplify-codegen": "2.13.0",
    "amplify-dynamodb-simulator": "1.13.0",
    "amplify-storage-simulator": "1.1.8",
    "chokidar": "^3.3.1",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.149",
    "@types/node": "^10.17.13",
    "aws-appsync": "^1.8.1",
    "aws-sdk": "^2.608.0",
    "axios": "^0.19.2",
    "graphql-auth-transformer": "6.13.0",
    "graphql-connection-transformer": "4.13.0",
    "graphql-dynamodb-transformer": "6.13.0",
    "graphql-function-transformer": "2.1.8",
    "graphql-key-transformer": "2.13.0",
    "graphql-tag": "^2.10.1",
    "graphql-transformer-core": "6.13.0",
    "graphql-versioned-transformer": "4.13.0",
    "isomorphic-fetch": "^2.2.1",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "jest-junit": "^8.0.0",
    "jsonwebtoken": "^8.5.1",
    "node-fetch": "^2.6.0",
    "ts-jest": "^24.0.2",
    "typescript": "~3.6.4",
    "uuid": "^3.4.0",
    "ws": "^7.2.1"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/__tests__/**"
    ],
    "reporters": [
      "default",
      "jest-junit"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost/",
    "testRegex": "(src/(__tests__|__e2e__)/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  },
  "jest-junit": {
    "outputDirectory": "reports/junit/",
    "outputName": "js-test-results.xml",
    "usePathForSuiteName": "true",
    "addFileAttribute": "true"
  }
}
