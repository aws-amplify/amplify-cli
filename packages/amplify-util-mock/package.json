{
  "name": "amplify-util-mock",
  "version": "2.0.0",
  "description": "amplify cli plugin providing local testing",
  "main": "lib/index.js",
  "scripts": {
    "e2e": "jest -w 4 src/__e2e__/*.e2e.test.ts",
    "test-watch": "jest --watch",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rm -rf ./lib",
    "prettier": "prettier --config ../../.prettierrc --write src/**/*.ts"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "amplify-appsync-simulator": "0.5.0",
    "amplify-category-function": "1.13.0",
    "amplify-codegen": "1.11.0",
    "amplify-dynamodb-simulator": "0.5.0",
    "amplify-provider-awscloudformation": "3.0.0",
    "amplify-storage-simulator": "0.5.0",
    "chokidar": "^3.0.0",
    "fs-extra": "^8.1.0",
    "lodash": "^4.17.15"
  },
  "devDependencies": {
    "@types/jest": "^24.0.16",
    "@types/lodash": "^4.14.129",
    "@types/node": "^8.10.51",
    "aws-amplify": "^1.1.36",
    "aws-sdk": "^2.510.0",
    "axios": "^0.19.0",
    "graphql-auth-transformer": "5.0.0",
    "graphql-connection-transformer": "3.11.0",
    "graphql-dynamodb-transformer": "5.0.0",
    "graphql-function-transformer": "1.7.0",
    "graphql-key-transformer": "1.7.0",
    "graphql-mapping-template": "3.7.0",
    "graphql-transformer-common": "3.14.0",
    "graphql-transformer-core": "5.0.0",
    "graphql-versioned-transformer": "3.10.0",
    "jest": "^24.8.0",
    "jest-environment-node": "^24.8.0",
    "node-fetch": "^2.6.0",
    "prettier": "^1.18.2",
    "ts-jest": "^24.0.2",
    "tslint": "^5.18.0",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "src/**/*.ts",
      "!**/node_modules/**",
      "!src/__tests__/**"
    ],
    "testEnvironment": "node",
    "testURL": "http://localhost/",
    "testRegex": "(src/(__tests__|__e2e__)/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
