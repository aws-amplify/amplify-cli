{
  "name": "graphql-versioned-transformer",
  "version": "5.0.0",
  "description": "A GraphQL transform that enabled object versioning and conflict resolution for a @model type.",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/graphql-versioned-transformer"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "clean": "rimraf ./lib"
  },
  "dependencies": {
    "graphql": "^14.5.8",
    "graphql-mapping-template": "4.18.4",
    "graphql-transformer-common": "4.20.0",
    "graphql-transformer-core": "7.0.0"
  },
  "devDependencies": {
    "@types/node": "^12.12.6",
    "graphql-dynamodb-transformer": "7.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testURL": "http://localhost",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
