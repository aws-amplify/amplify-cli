{
    "name": "@aws-amplify/graphql-model-transformer",
    "version": "0.4.2",
    "description": "Amplify graphql @model transformer",
    "repository": {
        "type": "git",
        "url": "https://github.com/aws-amplify/amplify-cli.git",
        "directory": "packages/amplify-graphql-model-transformer"
    },
