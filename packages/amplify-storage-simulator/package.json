{
  "name": "amplify-storage-simulator",
  "version": "0.5.0",
  "description": "An S3 simulator to test S3 APIs",
  "main": "lib/index.js",
  "scripts": {
    "test": "jest",
    "build": "tsc",
    "watch": "tsc -w",
    "prettier": "prettier --config ../../.prettierrc --write src/**/*.ts"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "dependencies": {
    "body-parser": "^1.19.0",
    "cors": "^2.8.5",
    "event-to-promise": "^0.8.0",
    "express": "^4.17.1",
    "glob": "^7.1.4",
    "ip": "^1.1.5",
    "lodash": "^4.17.15",
    "md5-file": "^4.0.0",
    "object-to-xml": "^2.0.0",
    "save": "^2.4.0",
    "serve-static": "^1.14.1",
    "xml": "^1.0.1",
    "xml-js": "^1.6.11"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/cors": "^2.8.5",
    "@types/glob": "^7.1.1",
    "@types/serve-static": "^1.13.2",
    "prettier": "1.18.2",
    "typescript": "^3.5.3"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "collectCoverage": true,
    "collectCoverageFrom": [
      "**/*.ts",
      "!**/node_modules/**",
      "!__tests__/**"
    ],
    "testURL": "http://localhost/",
    "testRegex": "(src/__tests__/.*.test.*)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "testPathIgnorePatterns": [
      "/node_modules/",
      "/lib/"
    ]
  }
}
