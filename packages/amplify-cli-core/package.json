{
  "name": "amplify-cli-core",
  "version": "3.5.0",
  "description": "Amplify CLI Core",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-cli-core"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "test": "jest --logHeapUsage",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo node_modules",
    "extract-api": "ts-node ../../scripts/extract-api.ts"
  },
  "dependencies": {
    "@aws-amplify/graphql-transformer-core": "^0.18.0",
    "@aws-amplify/graphql-transformer-interfaces": "^1.14.9",
    "ajv": "^6.12.6",
    "amplify-cli-logger": "1.2.2",
    "amplify-prompts": "2.6.2",
    "chalk": "^4.1.1",
    "ci-info": "^2.0.0",
    "cloudform-types": "^4.2.0",
    "dotenv": "^8.2.0",
    "execa": "^5.1.1",
    "fs-extra": "^8.1.0",
    "globby": "^11.0.3",
    "graphql-transformer-core": "^7.6.7",
    "hjson": "^3.2.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.21",
    "node-fetch": "^2.6.7",
    "open": "^8.4.0",
    "ora": "^4.0.3",
    "proxy-agent": "^5.0.0",
    "semver": "^7.3.5",
    "typescript-json-schema": "~0.52.0",
    "which": "^2.0.2"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/hjson": "^2.4.2",
    "@types/json-schema": "^7.0.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.6",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.0.0",
    "amplify-function-plugin-interface": "1.9.6",
    "nock": "^13.0.11",
    "rimraf": "^3.0.0",
    "strip-ansi": "^6.0.0",
    "uuid": "^8.3.2"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
