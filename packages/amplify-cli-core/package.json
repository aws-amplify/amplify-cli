{
  "name": "amplify-cli-core",
  "version": "1.21.2",
  "description": "Amplify CLI Core",
  "repository": {
    "type": "git",
    "url": "https://github.com/aws-amplify/amplify-cli.git",
    "directory": "packages/amplify-cli-core"
  },
  "author": "Amazon Web Services",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "types": "lib/index.d.ts",
  "keywords": [
    "graphql",
    "appsync",
    "aws"
  ],
  "scripts": {
    "test": "jest",
    "test-ci": "jest --ci -i",
    "build": "tsc",
    "watch": "tsc -w",
    "clean": "rimraf lib tsconfig.tsbuildinfo"
  },
  "dependencies": {
    "ajv": "^6.12.3",
    "amplify-cli-logger": "1.1.0",
    "ci-info": "^2.0.0",
    "cloudform-types": "^4.2.0",
    "dotenv": "^8.2.0",
    "fs-extra": "^8.1.0",
    "hjson": "^3.2.1",
    "js-yaml": "^4.0.0",
    "lodash": "^4.17.19",
    "node-fetch": "^2.6.1",
    "open": "^7.3.1",
    "semver": "^7.3.5"
  },
  "devDependencies": {
    "@types/fs-extra": "^8.0.1",
    "@types/hjson": "^2.4.2",
    "@types/json-schema": "^7.0.5",
    "@types/lodash": "^4.14.149",
    "@types/node": "^12.12.6",
    "@types/rimraf": "^3.0.0",
    "@types/uuid": "^8.0.0",
    "amplify-function-plugin-interface": "1.7.2",
    "nock": "^13.0.11",
    "rimraf": "^3.0.0"
  },
  "jest": {
    "transform": {
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(src/__tests__/.*.test.ts)$",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ],
    "collectCoverage": true
  }
}
